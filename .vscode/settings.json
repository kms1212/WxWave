{
    "[c]": {
        "editor.wordBasedSuggestions": "off",
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true,
        "editor.defaultFormatter": "xaver.clang-format",
        "editor.formatOnSaveMode": "modificationsIfAvailable",
        "editor.formatOnSave": true,
        "editor.indentSize": 4
    },
    "[cpp]": {
        "editor.wordBasedSuggestions": "off",
        "editor.suggest.insertMode": "replace",
        "editor.semanticHighlighting.enabled": true,
        "editor.defaultFormatter": "xaver.clang-format",
        "editor.formatOnSaveMode": "modificationsIfAvailable",
        "editor.formatOnSave": true,
        "editor.indentSize": 4
    },
    "files.associations": {
        "*.inc": "vasmmot",
        "*.asm": "vasmmot",
        "*.m4": "m4",
        "*.h++": "cpp",
        "cstdint": "cpp",
        "__bits": "cpp",
        "__config": "cpp",
        "__debug": "cpp",
        "__tuple": "cpp",
        "__verbose_abort": "cpp",
        "atomic": "cpp",
        "cstddef": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "exception": "cpp",
        "initializer_list": "cpp",
        "iosfwd": "cpp",
        "limits": "cpp",
        "memory": "cpp",
        "new": "cpp",
        "stdexcept": "cpp",
        "string": "cpp",
        "string_view": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "typeinfo": "cpp",
        "vector": "cpp",
        "__bit_reference": "cpp",
        "__hash_table": "cpp",
        "__locale": "cpp",
        "__node_handle": "cpp",
        "__split_buffer": "cpp",
        "__threading_support": "cpp",
        "__tree": "cpp",
        "array": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "cstdarg": "cpp",
        "ctime": "cpp",
        "execution": "cpp",
        "ios": "cpp",
        "locale": "cpp",
        "map": "cpp",
        "mutex": "cpp",
        "optional": "cpp",
        "ratio": "cpp",
        "streambuf": "cpp",
        "unordered_map": "cpp",
        "variant": "cpp",
        "algorithm": "cpp",
        "bitset": "cpp",
        "deque": "cpp",
        "hash_map": "cpp",
        "list": "cpp",
        "queue": "cpp",
        "set": "cpp",
        "span": "cpp",
        "stack": "cpp",
        "format": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "chrono": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory_resource": "cpp",
        "random": "cpp",
        "utility": "cpp",
        "unistd.h": "c"
    },
    "docwriter.style": "Doxygen",
    "docwriter.language": "English",
    "cmake.configureOnOpen": false,
    "cmake.configureEnvironment": {
        "ZLIB_ROOT": "/opt/homebrew/opt/zlib",
        "LibXml2_ROOT": "/opt/homebrew/opt/libxml2"
    },
    "C_Cpp.default.compilerPath": "/opt/homebrew/opt/llvm/bin/clang",
}